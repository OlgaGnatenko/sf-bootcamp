    <validationRules>
        <fullName>Check_HR_Manager_Address_for_US_location</fullName>
        <active>true</active>
        <description>Check HR Manager Address for US Location</description>
        <errorConditionFormula>AND(  ISPICKVAL(Location__c, &quot;USA&quot;), NOT(
OR(
HR_Manager__r.MailingCountry == &quot;US&quot;,
HR_Manager__r.MailingCountry == &quot;USA&quot;,
HR_Manager__r.MailingCountry == &quot;United States&quot;
)
))</errorConditionFormula>
        <errorDisplayField>HR_Manager__c</errorDisplayField>
        <errorMessage>For location &quot;USA&quot;, HR Manager&apos;s Mailing Country should be US, USA or United States</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Open_Due_Date_for_new_records</fullName>
        <active>true</active>
        <description>Open Due Date for new records cannot be in the past</description>
        <errorConditionFormula>AND(ISNEW(), Open_Due_Date__c &lt;  TODAY())</errorConditionFormula>
        <errorDisplayField>Open_Due_Date__c</errorDisplayField>
        <errorMessage>Open Due Date of a new Job Position record cannot be in the past</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Location_Only_For_Nonremote_Work</fullName>
        <active>true</active>
        <description>Require Location Only For Nonremote Work</description>
        <errorConditionFormula>AND( Willing_to_work_remote__c = False,  ISBLANK(TEXT(Location__c)))</errorConditionFormula>
        <errorDisplayField>Location__c</errorDisplayField>
        <errorMessage>Location is mandatory for non-remote Job Positions.</errorMessage>
    </validationRules>
